{
  "name": "all_contacts Table",
  "inherits": "@budibase/standard-components/table",
  "props": {
    "data": {
      "##bbstate": "/all_contacts",
      "##bbsource": "store"
    },
    "tableClass": "table table-hover",
    "theadClass": "thead-dark",
    "columns": [
      {
        "title": "contacted",
        "value": {
          "##bbstate": "contacted",
          "##bbsource": "context"
        }
      },
      {
        "title": "name",
        "value": {
          "##bbstate": "name",
          "##bbsource": "context"
        }
      }
    ],
    "onRowClick": [
      {
        "##eventHandlerType": "Set State",
        "parameters": {
          "path": "currentView",
          "value": {
            "##bbstate": "type",
            "##bbsource": "context"
          }
        }
      }
    ]
  }
}
