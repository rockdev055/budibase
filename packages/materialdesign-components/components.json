      "button2Click": "event"      
    }
  },
  "Card": {
    "name": "Card",
    "description": "A Material Card container. Accepts CardHeader, CardBody and CardFooter as possible children",
    "props": {
      "width": {"type": "string", "default": "350px"},
      "height": "string",
      "variant": {
        "type": "options",
